FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


simulationType   RAS;

RAS
{
    RASModel        kEpsilon;
    kEpsilonCoeffs
    {
   
    Cmu              0.09;
    C1               1.44;
    C2               1.92;
    C3               -0.33;
    alphah           1;
    alphak           1;
    alphaEps         0.76923;
    }

    turbulence      on;

    printCoeffs     on;
}


