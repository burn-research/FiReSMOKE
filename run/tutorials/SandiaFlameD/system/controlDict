FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     firePimpleSMOKE;

startFrom       latestTime;

startTime       0;

stopAt		endTime;

endTime         4.5;

deltaT          1.e-5;

writeControl    runTime;

writeInterval   5e-3;

adjustTimeStep	false;

maxCo		0.4;

purgeWrite      5;

writeFormat     ascii;

writePrecision  6;

writeCompression on;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable true;

