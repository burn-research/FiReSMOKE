FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     edcPimpleSMOKE_7_dynamic;

startFrom       latestTime;

startTime       0;

stopAt		endTime;

endTime         1.5;

deltaT          1.e-5;

adjustTimeStep	false;

maxCo		0.2;

writeControl    runTime;

writeInterval   1.e-2;

purgeWrite      2;

writeFormat     ascii;

writePrecision  6;

writeCompression on;

timeFormat      general;

timePrecision   6;

graphFormat     raw;

runTimeModifiable true;



